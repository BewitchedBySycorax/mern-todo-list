{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2020": true,
		"node": true
	},
	"extends": "airbnb-base",
	"parserOptions": { 
		"ecmaVersion": 11
	},
	"rules": {
		"arrow-parens": [2, "as-needed"],
		"comma-dangle": [2, "never"],
		"consistent-return":"off",
		"func-names":"off",
		"no-confusing-arrow": "off",
		"no-console":"off",
		"no-underscore-dangle": "off",
		"padded-blocks":"off",
		"strict": [2, "global"],
		"semi": [2, "never"]	
	}
}
